<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="PaymentScreenCustom" t-inherit="point_of_sale.PaymentScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('payment-buttons')]" position="inside">
            
            <div class="payment-button">
                <div class="button" t-att-class="highlight">
                    <i class="fa fa-file-text-o" /> Complemento de Pago                        
                </div>
            </div>
           
        </xpath>
    </t>
</templates>
